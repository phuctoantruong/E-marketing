      name="SEO Management"
      res_model="emarketingapp.seo"
      view_mode="tree,form" />
      
	<!-- Menu Items -->
	<menuitem id="menu_emarketing_top"
      name="E-marketing Management" sequence="40"/>
      
    <menuitem id="menu_object_management_root"
      name="Object Management" parent="menu_emarketing_top" />

    <menuitem id="menu_seo_management"
      name="SEO"
      parent="menu_object_management_root"
      sequence="10"
